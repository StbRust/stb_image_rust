	"version": "0.2.0",
	"configurations": [
		{
			"type": "lldb",
			"request": "launch",
			"name": "testing debug executable",
			"cargo": {
				"args": [
					"build",
					"--bin=testing",
					"--package=testing",
				],
				"filter": {
					"name": "testing",
					"kind": "bin",
				},
			},
			"args": [],
			"cwd": "${workspaceFolder}",
			"env": {
				"RUST_LOG": "error",
			},
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug unit tests in executable 'testing'",
			"cargo": {
				"args": [
					"test",
					"--no-run",
					"--bin=testing",
					"--package=testing",
				],
				"filter": {
					"name": "testing",
					"kind": "bin",
				},
			},
			"args": [],
			"cwd": "${workspaceFolder}",
		},
	],
}